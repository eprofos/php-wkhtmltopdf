{
    "name": "eprofos/php-wkhtmltopdf",
    "description": "A PHP library for converting HTML to PDF using wkhtmltopdf with extensive customization options.",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "symfony/process": "^4.4 || ^5.0 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0 || ^6.0",
        "symfony/filesystem": "^4.4 || ^5.0 || ^6.0",
        "symfony/finder": "^4.4 || ^5.0 || ^6.0",
        "symfony/options-resolver": "^4.4 || ^5.0 || ^6.0",
        "symfony/stopwatch": "^4.4 || ^5.0 || ^6.0",
        "symfony/string": "^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^2.19 || ^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eprofos\\PhpWkhtmltopdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eprofos\\PhpWkhtmltopdf\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Houssem TAYECH",
            "email": "houssem@eprofos.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "tags": ["pdf", "html", "conversion", "wkhtmltopdf", "php-library"]
    },
    "scripts": {
        "test": "phpunit",
        "fix": "php-cs-fixer fix"
    },
    "keywords": ["pdf", "html", "wkhtmltopdf", "php", "conversion"],
    "support": {
        "issues": "https://github.com/eprofos/php-wkhtmltopdf/issues",
        "source": "https://github.com/eprofos/php-wkhtmltopdf"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eprofos/php-wkhtmltopdf"
        }
    ]
}
